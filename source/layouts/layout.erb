<!doctype html>
<html ng-app="fiftyTerms">
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "50 Term | Psychology Terms Explained" %></title>

    <script type="text/javascript" src="bower_components/angular/angular.min.js"></script>

    <%= stylesheet_link_tag :site %>
    <%= javascript_include_tag :'app/app' %>
    <%= javascript_include_tag :'app/dataService' %>
    <%= javascript_include_tag :'app/appController' %>
  </head>

  <body class="<%= page_classes %>" ng-controller="appController as app">
    <%= yield %>
  </body>
</html>
